{
    "version": 2,
    "public": false,
    "builds": [
      { "src": "/client/package.json", "use": "@now/static-build" },
      { "src": "server.js", "use": "@now/node-server" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "server.js" },
      { "src": "^/static/(.*)", "dest": "/static/$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
  }